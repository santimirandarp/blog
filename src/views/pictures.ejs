<!DOCTYPE html>
<html>
	<%- include("includes/head", {title, page,cssPath}) -%>
	<body>
		<!--this Include is usful for reusing pieces of HTML-->
		<%- include("includes/navbar",navigationLinks) %>

		<div class='banner'>
		<h1> <%= title %> </h1>
		</div>

		<main>
<div class='imgGal'>
               <% images.forEach(img => { %>
               <%const text= img.replace(/_/g, " ").split('.')[0]%>
               <a href="gallery/<%= img %>">
	       <figure class="pictures">
	       <img src='/gallery/<%=img%>' alt='<%= text %>'/>
	       <figcaption><%= text %></figcaption>
	       </figure></a>
	       <%})%>
</div>
		</main>

		<%- include('includes/footer')%>
		<!--		<script src="./javascripts/addCheckMarks.js"></script> -->
	</body>
</html>
